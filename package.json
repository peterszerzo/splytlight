{
  "name": "splytlight",
  "version": "0.1.0",
  "description": "Visualization experiments for Splyt Light",
  "main": "src/index.js",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --hot --history-api-fallback --inline --open --content-base build/ --port 3000",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack",
    "deploy": "surge ./build splytlight.surge.sh",
    "all": "npm build && npm run deploy"
  },
  "author": "Peter Szerzo",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "typecheck",
        {
          "disable": {
            "production": true
          }
        }
      ],
      "syntax-flow",
      "transform-flow-strip-types"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "classnames": "^2.2.5",
    "domready": "^1.0.8",
    "three": "^0.79.0",
    "three-orbit-controls": "^72.0.0",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.13.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-plugin-babel": "^3.3.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "postcss": "^5.1.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-validator": "^2.2.7"
  }
}
