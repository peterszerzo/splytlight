{
  "name": "splytlight",
  "version": "0.2.0",
  "description": "Visualization experiments for Splyt Light",
  "author": "Peter Szerzo, Scott Leinweber",
  "private": true,
  "devDependencies": {
    "prettier": "^1.15.0",
    "react-scripts": "^3.3.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "emotion-theming": "^10.0.19",
    "marked": "^0.3.6",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "redux": "^4.0.0",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.0.0",
    "three": "^0.83.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier './src/**/*.js' --write",
    "deploy": "npm run build && git branch -D gh-pages && git checkout --orphan gh-pages && trash src public && mv build/* . && git add . && git commit -m 'Deploy' && git push origin gh-pages && git checkout master"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
